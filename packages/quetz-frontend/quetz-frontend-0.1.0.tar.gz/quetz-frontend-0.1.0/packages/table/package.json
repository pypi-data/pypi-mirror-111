{
  "name": "@quetz-frontend/table",
  "version": "0.1.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "styleModule": "style/index.js",
  "directories": {
    "lib": "lib/"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
