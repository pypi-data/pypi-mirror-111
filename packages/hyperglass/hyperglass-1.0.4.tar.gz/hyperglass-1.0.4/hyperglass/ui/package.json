    "@hookstate/core": "^3.0.7",
    "@hookstate/persistence": "^3.0.0",
    "@meronex/icons": "^4.0.0",
    "dagre": "^0.8.5",
    "dayjs": "^1.10.4",
    "framer-motion": "^4.1.17",
    "lodash": "^4.17.21",
    "next": "^10.2.3",
    "palette-by-numbers": "^0.1.5",
    "react": "^17.0.2",
    "react-countdown": "^2.2.1",
    "react-device-detect": "^1.15.0",
