test_calc_digest_products:
  -
    seq: 'GAATTCAAGCTT'
    enzymes: ['EcoRI']
    is_circular: True
    target_size: None
    products: ['AATTCAAGCTTG']
    product: 'AATTCAAGCTTG'

  -
    seq: 'GAATTCAAGCTT'
    enzymes: ['HindIII']
    is_circular: True
    target_size: None
    products: ['AGCTTGAATTCA']
    product: 'AGCTTGAATTCA'

  -
    seq: 'GAATTCAAGCTT'
    enzymes: ['HindIII']
    is_circular: False
    target_size: None
    products: ['GAATTCA', 'AGCTT']
    product: 'GAATTCA'

  -
    seq: 'GAATTCAAGCTT'
    enzymes: ['HindIII']
    is_circular: False
    target_size: 7
    products: ['GAATTCA', 'AGCTT']
    product: 'GAATTCA'

  -
    seq: 'GAATTCAAGCTT'
    enzymes: ['HindIII']
    is_circular: False
    target_size: 5
    products: ['GAATTCA', 'AGCTT']
    product: 'AGCTT'

  -
    seq: 'GAATTCaAAGCTT'
    enzymes: ['EcoRI', 'HindIII']
    is_circular: True
    target_size: None
    products: ['AATTCaA', 'AGCTTG']
    product: 'AATTCaA'

  -
    seq: 'GAATTCaAAGCTT'
    enzymes: ['EcoRI', 'HindIII']
    is_circular: True
    target_size: 6
    products: ['AATTCaA', 'AGCTTG']
    product: 'AGCTTG'

  -
    seq: 'GAATTCaAAGCTT'
    enzymes: ['EcoRI', 'HindIII']
    is_circular: False
    target_size: None
    products: ['G', 'AATTCaA', 'AGCTT']
    product: 'AATTCaA'

  -
    seq: 'GAATTCaAAGCTT'
    enzymes: ['EcoRI', 'HindIII']
    is_circular: False
    target_size: 1
    products: ['G', 'AATTCaA', 'AGCTT']
    product: 'G'

  -
    seq: 'GAATTCaAAGCTT'
    enzymes: ['EcoRI', 'HindIII']
    is_circular: False
    target_size: 5
    products: ['G', 'AATTCaA', 'AGCTT']
    product: 'AGCTT'

  -
    id: err
    seq: 'GAATTCGAATTC'
    enzymes: []
    is_circular: False
    target_size: None
    error:
      type: UsageError
      message: no enzymes specified
  -
    id: err
    seq: 'GAATTCGAATTC'
    enzymes: ['HindIII']
    is_circular: False
    target_size: None
    error:
      type: ConfigError
      message: 'HindIII' does not cut template
  -
    id: err
    seq: 'GAATTCGAATTC'
    enzymes: ['HindIII', 'BsaI']
    is_circular: False
    target_size: None
    error:
      type: ConfigError
      message: 'HindIII','BsaI' do not cut template
  -
    id: err
    seq: 'GAATTCGAATTC'
    enzymes: ['unknown enzyme']
    is_circular: False
    target_size: None
    error:
      type: ConfigError
      message: unknown enzyme\(s\): 'unknown enzyme'

